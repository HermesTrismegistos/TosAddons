<?xml version="1.0" encoding="UTF-8"?>
<uiframe name="developerconsole" x="0" y="0" width="800" height="500">
    <frame layout_gravity="left bottom"/>
	<input hittest="true" movable="true" moveintitlebar="false" hittestframe="true"  resize="true" resizerange="60 30 1800 1000"/>
    <option hideable="true"/>
    <draw blend="95"/>
	<script OpenScp="DEVELOPERCONSOLE_OPEN" CloseScp="DEVELOPERCONSOLE_CLOSE"/>
    <controls>
        <textview name="textview_log" rect="10 10 675 435" transparent="false"/>
		<edit name="input" rect="10 450 675 40" fontname="white_16_ol" EnterKeyScp="DEVELOPERCONSOLE_ENTER_KEY" typingsound="chat_typing"/>
		
		<button name="execute" rect="690 450 100 40" caption="Enter"/>
        
        <timer name="addontimer" rect="0 0 10 10"/>

        <button name="resize" rect="0 0 30 30" margin="0 3 0 0" layout_gravity="right top" MouseOffAnim="btn_mouseoff" MouseOnAnim="btn_mouseover" image="button_chat_scale" oversound="button_over" resizeframe="true" texttooltip="{@st59}채팅창 크기 조절{/}"/>
		
    </controls>
</uiframe>
